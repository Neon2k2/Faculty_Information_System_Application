@model IEnumerable<Faculty_Information_System_MVC_Layer.Models.CourseTaughtVM>

@{
    ViewData["Title"] = "ShowCourTaught";
}

<link rel="stylesheet" href="~/Style/StyleSheet3.css" />

<p>
    <a asp-action="CourTautcreate">Add Entry of Courses Taught</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CoureseTaughtId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CourseId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FacultyId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SubjectId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FirstDateTaught)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CoureseTaughtId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CourseId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FacultyId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SubjectId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstDateTaught)
            </td>
            <td>
                @Html.ActionLink("Update", "CourTautedit", new { id=item.CoureseTaughtId }) |
                @Html.ActionLink("Delete", "CourDelete", new { id=item.CoureseTaughtId })
            </td>
        </tr>
}
    </tbody>
</table>





<div>
    <a asp-action="Index">Back to Home</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
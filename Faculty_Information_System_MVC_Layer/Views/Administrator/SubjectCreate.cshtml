@model Faculty_Information_System_MVC_Layer.Models.SubjectVM

@{
    ViewData["Title"] = "SubjectCreate";
}


<link rel="stylesheet" href="~/Style/StyleSheet3.css" />

<h4>SubjectVM</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="SubjectCreate">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="SubjectName" class="control-label"></label>
                <input asp-for="SubjectName" class="form-control" />
                <span asp-validation-for="SubjectName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <p>
    <button class="rounded-left btn-dark"><a asp-action="ShowSubject" class="text-decoration-none text-white">Back to Subjects</a></button>
<br/></p>

</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

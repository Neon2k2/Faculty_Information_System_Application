@model IEnumerable<Faculty_Information_System_MVC_Layer.Models.PublicationVM>




<p>
    <button class="rounded-left btn-dark"><a asp-action="PublishCreate" class="text-decoration-none text-white">Add new Publication</a></button>
<br/></p>


@{
    ViewData["Title"] = "ShowPublish";
}

<link rel="stylesheet" href="~/Style/StyleSheet3.css" />



<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PublicationId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FacultyId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PublicationTitle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ArticleName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PublisherName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PublisherLocation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CitationDate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PublicationId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FacultyId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PublicationTitle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ArticleName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PublisherName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PublisherLocation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CitationDate)
            </td>
            <td>
                @Html.ActionLink("Update", "PublishEdit", new { id=item.PublicationId })|
                @Html.ActionLink("Delete", "PublishDelete", new { id=item.PublicationId })
            </td>
        </tr>
}
    </tbody>
</table>

<div>
    <p>
    <button class="rounded-left btn-dark"><a asp-action="Index" class="text-decoration-none text-white">Back to Home</a></button>
<br/></p>

</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
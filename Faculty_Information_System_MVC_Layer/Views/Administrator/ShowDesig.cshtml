@model IEnumerable<Faculty_Information_System_MVC_Layer.Models.DesignationVM>




<p>
    <button class="rounded-left btn-dark"><a asp-action="DesigCreate" class="text-decoration-none text-white">Add New Designation</a></button>
<br/></p>


<link rel="stylesheet" href="~/Style/StyleSheet3.css" />
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DesignationId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DesignationName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DesignationId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DesignationName)
            </td>
            <td>
                @Html.ActionLink("Update", "DesigEdit", new {  id=item.DesignationId  }) |

                @Html.ActionLink("Delete", "DesigDelete", new { id=item.DesignationId})
            </td>
        </tr>
}
    </tbody>
</table>

<div>
    <p>
    <button class="rounded-left btn-dark"><a asp-action="Index" class="text-decoration-none text-white">Back to Home</a></button>
<br/></p>

</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}